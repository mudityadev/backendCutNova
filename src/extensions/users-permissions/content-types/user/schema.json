{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "bio": {
      "type": "richtext"
    },
    "isBusinessAccount": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "businessDetail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::business-detail.business-detail",
      "mappedBy": "user"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "user"
    },
    "comment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::comment.comment",
      "mappedBy": "user"
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::appointment.appointment",
      "inversedBy": "user"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "user"
    },
    "likedPosts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "inversedBy": "users"
    },
    "postsLikes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "likes"
    }
  }
}
