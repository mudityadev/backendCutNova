{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "Appointment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service",
      "inversedBy": "appointment"
    },
    "appointmentDate": {
      "type": "date"
    },
    "appointmentTime": {
      "type": "time"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Confirmed",
        "Cancelled",
        "Completed"
      ]
    },
    "createdDate": {
      "type": "date"
    },
    "updatedDate": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "appointments"
    }
  }
}
