{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "quantities": {
      "type": "json"
    },
    "totalPrice": {
      "type": "decimal"
    },
    "shippingAddress": {
      "type": "json"
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Paid",
        "Refunded"
      ]
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Processing",
        "Shipped",
        "Delivered",
        "Cancelled"
      ]
    },
    "createdDate": {
      "type": "date"
    },
    "updatedDate": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "orders"
    }
  }
}
